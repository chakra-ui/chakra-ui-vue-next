    "@chakra-ui/utils": "2.0.14",
    "@chakra-ui/vue-composables": "workspace:*",
    "@chakra-ui/vue-system": "workspace:*",
    "@chakra-ui/vue-utils": "workspace:*",
    "@vueuse/motion": "^1.5.4",
    "@vueuse/shared": "^9.11.1"
  },
  "devDependencies": {
