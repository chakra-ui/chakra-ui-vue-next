{
  "name": "@chakra-ui/c-pin-input",
  "description": "Chakra UI Vue | The pin input component is similar to the input component but is optimized for entering sequences of digits quickly component",
  "version": "2.1.0-beta.9",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "author": "Shyrro <zsahmane@gmail.com>",
  "homepage": "https://github.com/chakra-ui/chakra-ui-vue-next#readme",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "repository": "https://github.com/chakra-ui/chakra-ui-vue-next/tree/master/packages/c-pin-input",
  "bugs": {
    "url": "https://github.com/chakra-ui/chakra-ui-vue-next/issues"
  },
  "scripts": {
    "clean": "rimraf dist .turbo",
    "build": "tsup && pnpm build:types",
    "build:fast": "tsup",
    "types:check": "tsc --noEmit",
