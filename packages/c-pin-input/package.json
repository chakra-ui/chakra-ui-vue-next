    "build:types": "tsup src --dts-only"
  },
  "dependencies": {
    "@chakra-ui/c-input": "workspace:*",
    "@chakra-ui/styled-system": "2.6.2",
    "@chakra-ui/utils": "2.0.15",
    "@chakra-ui/vue-system": "workspace:*",
    "@chakra-ui/vue-utils": "workspace:*",
    "@zag-js/pin-input": "0.2.12",
    "@zag-js/vue": "0.2.10",
    "csstype": "^3.1.1"
  },
  "devDependencies": {
    "vue": "3.2.47"
  },
  "peerDependencies": {
    "vue": "3.2.47"
  },
  "publishConfig": {
    "access": "public"
  }
