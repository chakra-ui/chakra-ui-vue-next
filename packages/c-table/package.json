  "version": "2.0.0-beta.8",
  "author": "Jonathan Bakebwa <codebender828@gmail.com>",
  "homepage": "https://github.com/chakra-ui/chakra-ui-vue-next#readme",
  "license": "MIT",
  "main": "./index.ts",
  "clean-package": "../../clean-package.config.json",
  "typings": "dist/index.d.ts",
  "files": [
