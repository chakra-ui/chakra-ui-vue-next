{
  "name": "@chakra-ui/vue-styled",
  "version": "2.1.0-beta.8",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "description": "Chakra UI Vue | Styled module",
  "repository": "https://github.com/chakra-ui/chakra-ui-vue-next/tree/master/packages/styled",
  "author": "Jonathan Bakebwa codebender828@gmail.com",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "build": "tsup && pnpm build:types",
    "build:fast": "tsup",
    "types:check": "tsc --noEmit",
    "dev": "concurrently 'tsup --watch' 'tsup src --dts-only --watch'",
    "build:types": "tsup src --dts-only"
  },
  "dependencies": {
    "@chakra-ui/vue-utils": "workspace:*",
    "@emotion/cache": "^11.10.5",
    "@emotion/css": "^11.10.5",
    "@emotion/serialize": "1.1.1",
    "@emotion/utils": "^1.1.0",
    "@emotion/weak-memoize": "^0.2.5",
    "clsx": "^1.1.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.memoize": "^4.1.2"
  },
  "devDependencies": {
    "@types/lodash.memoize": "^4.1.6",
    "vue": "3.2.47"
  },
  "peerDependencies": {
    "vue": "3.2.47"
  },
  "publishConfig": {
    "access": "public"
  }
